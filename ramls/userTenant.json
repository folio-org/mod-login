{
  "title": "User Tenant Association Schema",
  "type": "object",
  "description": "An entity that describes an association between a user and a tenant in a consortium",
  "properties": {
    "id": {
      "description": "identifier",
      "type": "string"
    },
    "userId": {
      "description": "User identifier",
      "type": "string"
    },
    "username": {
      "description": "Username",
      "type": "string"
    },
    "tenantId": {
      "description": "Tenant identifier",
      "type": "string"
    },
    "tenantName": {
      "description": "Tenant name",
      "type": "string"
    },
    "isPrimary": {
      "description": "True if it is the primary tenant for the user",
      "type": "boolean"
    }
  }
}
