{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Credentials existence",
  "type": "object",
  "properties": {
    "credentialsExist": {
      "type": "boolean",
      "description": "True if user has local password"
    },
    "isPlaceholder": {
      "type": "boolean",
      "description": "True if credentials record is a placeholder",
      "default": false
    }
  },
  "additionalProperties": false,
  "required": [
    "credentialsExist"
  ]
}
